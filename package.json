{
  "name": "property-accessor",
  "version": "2.1.1",
  "description": "This package evaluates path to nested object properties like 'prop1[0].prop2[9]['name']' and returns value ",
  "keywords": [
    "proxy",
    "property accessor",
    "evaluate path"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Pavlo Lazunko",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc && tsc-alias && eslint src/**/*.ts{,x}",
    "eslint": "eslint src --ext .ts",
    "prettier-check": "prettier . --check",
    "prettier-write": "prettier . --write",
    "test": "node test/test.js",
    "runtime": "node dist/index.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "20.10.6",
    "@types/prettier": "2.6.0",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "eslint": "8.47.0",
    "husky": "3.0.0",
    "prettier": "3.3.3",
    "prettier-plugin-multiline-arrays": "3.0.6",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.10",
    "typescript": "5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Paul-Lazunko/property-accessor.git"
  }
}
